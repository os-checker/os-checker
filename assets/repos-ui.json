{
  "arceos-org/arceos": {
    "targets": [
      "x86_64-unknown-linux-gnu"
    ],
    "packages": {
      "bwbench-client": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "deptool": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "arceos-shell": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "arceos-httpserver": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "arceos-httpclient": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "arceos-helloworld": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "mingo": {
        "targets": "aarch64-unknown-none-softfloat"
      }
    }
  },
  "arceos-org/axplat_crates": {
    "packages": {
      "cargo-axplat": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "axplat-riscv64-qemu-virt": {
        "targets": "riscv64gc-unknown-none-elf"
      },
      "axplat-aarch64-bsta1000b": {
        "targets": "aarch64-unknown-none"
      },
      "axplat-aarch64-phytium-pi": {
        "targets": "aarch64-unknown-none"
      },
      "axplat-aarch64-qemu-virt": {
        "targets": "aarch64-unknown-none"
      },
      "axplat-aarch64-raspi": {
        "targets": "aarch64-unknown-none"
      },
      "axplat-aarch64-peripherals": {
        "targets": "aarch64-unknown-none"
      },
      "axplat-loongarch64-qemu-virt": {
        "targets": "loongarch64-unknown-none"
      },
      "axplat-x86-pc": {
        "targets": "x86_64-unknown-none"
      }
    }
  },
  "drivercraft/fdt-parser": {
    "packages": {
      "dtb-tool": {
        "targets": "x86_64-unknown-linux-gnu"
      }
    }
  },
  "rcore-os/arm-gic-driver": {
    "targets": "aarch64-unknown-none-softfloat"
  },
  "asterinas/asterinas": {
    "setup": [
      "git clone https://github.com/asterinas/linux_vdso /tmp/linux_vdso",
      "make build"
    ],
    "env": {
      "VDSO_LIBRARY_DIR": "/tmp/linux_vdso"
    },
    "meta": {
      "skip_pkg_dir_globs": [
        "comp-sys*",
        "examples_in_book*"
      ]
    },
    "packages": {
      "cargo-osdk": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "sctrace": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "linux-bzimage-builder": {
        "targets": "x86_64-unknown-linux-gnu"
      }
    }
  },
  "drivercraft/CrabUSB": {
    "packages": {
      "usb-keyboard": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "uvc-frame-parser": {
        "targets": "x86_64-unknown-linux-gnu"
      },
      "test_libusb_uvc": {
        "targets": "x86_64-unknown-linux-gnu"
      }
    }
  }
}
